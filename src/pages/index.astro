import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="id">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header />
		<main>
			<h1>Home.or.id Site</h1>
			<p>
				Selamat datang di website Home.or.id.
			</p>
           <p>
<img src="https://home.or.id/home.png" alt="home.or.id">
			</p>
			
		</main>
		<Footer />
	</body>
</html>
